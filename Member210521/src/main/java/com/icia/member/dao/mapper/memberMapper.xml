<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
 <mapper namespace="member"> 
	<insert id = "memberjoininsertDB" parameterType="memberdto">
		INSERT INTO MEMBER210521(mid,mpassword,mname,memail) values(#{mid},#{mpassword},#{mname},#{memail})
	</insert>
	
	<select id="memberlistDB" resultType="memberdto">
		select * from member210521
	</select>
	
 </mapper>